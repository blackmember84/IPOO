#include<iostream>
using namespace std;

int main(){
	float arr[7];
	for(int i=0;i<7;i++){
		cin>>arr[i];
	}
	float min=arr[0];
	float max=arr[0];
	for(int j=1;j<7;j++){
		if(arr[j]<min){
			min=arr[j];
		}
		if(arr[j]>max){
			max=arr[j];
		}
	
	}
	cout<<"el minimo es "<<min<<","<<"el maximo es "<<max<<"\n";
}
